        </activity>

        <activity
            android:name=".AgencyListActivity_"
            android:label="@string/title_agencies_main" >

        </activity>

        <activity
            android:name=".AgencyDetailActivity_"
            android:label="@string/title_agencies_main" >

        </activity>


    </application>

</manifest>